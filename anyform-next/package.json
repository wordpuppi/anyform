{
  "name": "@wordpuppi/anyform-next",
  "version": "0.5.1",
  "description": "Next.js integration for anyform - RSC support and Server Actions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@wordpuppi/anyform-react": "file:../anyform-react"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.0.0",
    "next": "^14.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wordpuppi/anyform"
  },
  "keywords": [
    "nextjs",
    "react",
    "forms",
    "rsc",
    "server-actions",
    "anyform"
  ],
  "license": "MIT"
}
